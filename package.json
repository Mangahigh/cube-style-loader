{
  "name": "prodigi-cube-style-loader",
  "version": "0.0.2",
  "description": "Webpack style loader for prodigi3",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=6.5",
    "npm": ">=3.10",
    "yarn": ">=1.2"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "dependencies": {
    "hoist-non-react-statics": "2.3.1",
    "loader-utils": "^1.1.0",
    "prop-types": "15.6.0",
    "react": "16.1.1",
    "react-dom": "16.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.1",
    "coveralls": "^2.13.1",
    "create-react-class": "^15.6.0",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.2.0",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom": "^11.1.0",
    "mocha": "^3.5.0",
    "rimraf": "^2.6.1",
    "sinon": "^3.2.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "stage-2",
      "react"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul",
          "rewire",
          "dynamic-import-webpack"
        ]
      }
    }
  },
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "editorconfig-tools fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,less,scss,sss}": [
      "stylefmt",
      "stylelint",
      "git add"
    ]
  },
  "scripts": {
    "build": "rimraf lib && babel src --out-dir lib"
  }
}
